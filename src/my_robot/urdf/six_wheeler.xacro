<?xml version="1.0" ?>

<robot name="six_wheeler" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:include filename="$(find my_robot)/urdf/six_wheeler.gazebo" />

    <link name="robot_footprint"></link>

    <joint name="robot_footprint_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="robot_footprint" />
        <child link="chassis" />
    </joint>

    <link name="chassis">
        <pose>0 0 0.1 0 0 0</pose>

        <inertial>
            <mass value="25.0" />
            <origin xyz="0.0 0 0" rpy=" 0 0 0" />
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>

        <collision name="chassis_collision">
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <box size=".8 .2 .1" />
            </geometry>
        </collision>

        <visual name="chassis_visual">
            <origin xyz="0 0 0" rpy=" 0 0 0" />
            <geometry>
                <box size=".8 .2 .1" />
            </geometry>
        </visual>
    </link>

    <!-- #region left_wheel_1 -->
    <link name="left_wheel_1">
        <inertial>
            <mass value="5.0" />
            <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707" />
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
        </inertial>

        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.075" length="0.05" />
            </geometry>
        </collision>

        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707" />
            <geometry>
                <cylinder radius="0.075" length="0.05" />
            </geometry>
        </visual>
    </link>

    <joint type="continuous" name="left_wheel_1_hinge">
        <origin xyz="0.25 0.13 0" rpy="0 0 0" />
        <child link="left_wheel_1" />
        <parent link="chassis" />
        <axis xyz="0 1 0" rpy="0 0 0" />
        <limit effort="10000" velocity="1000" />
        <dynamics damping="1.0" friction="1.0" />
    </joint>

    <gazebo reference="left_wheel_1">
        <material>Gazebo/Green</material>
    </gazebo>
    <!-- #endregion left_wheel_1 -->

    <!-- #region left_wheel_2 -->
    <link name="left_wheel_2">
        <inertial>
            <mass value="5.0" />
            <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707" />
